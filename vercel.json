{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(docs|redoc|openapi\\.json)",
      "dest": "api/index.py"
    },
    {
      "src": "/frontend/(.*\\.(css|js|html|ico|png|jpg|svg|webp))",
      "dest": "/frontend/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/frontend/?$",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/frontend/",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "^/$",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}
